{
  "name": "@clobwise/crm-front",
  "version": "0.1.0",
  "description": "Frontend for CRM",
  "type": "module",
  "scripts": {
    "check:types": "tsc -b tsconfig.json",
    "build": "rspack build --env production",
    "dev": "rspack dev --env development",
    "dev:gql": "npm run dev:gqlGen && npm run dev:gqlFormat",
    "dev:gqlGen": "NODE_OPTIONS='--import tsx' graphql-codegen --c ./script/Codegen.ts",
    "dev:gqlFormat": "prettier --write ./src/api",
    "dev:format": "prettier --write ."
  },
  "imports": {
    "#api/*": "./src/api/*",
    "#shared/*": "./src/shared/*"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "volta": {
    "node": "22.5.1"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.36.1",
    "@spectrum-icons/ui": "^3.6.9",
    "@spectrum-icons/workflow": "^4.2.14",
    "@tanstack/react-form": "^0.21.0",
    "@tanstack/react-query": "^5.51.21",
    "@tanstack/react-router": "^1.45.11",
    "clsx": "^2.1.1",
    "gql.tada": "^1.7.5",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "ky": "^1.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "tsx": "^4.17.0",
    "typescript": "^5.4.5"
  }
}
